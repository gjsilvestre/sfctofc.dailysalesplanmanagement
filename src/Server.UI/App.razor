    <script>
        let reportWindow = null;

        // // open new window and return reference id
        // window.openBlankWindow = () => {
        //     return window.open("", "_blank");
        // };

        // // write HTML into an existing window
        // window.writeHtmlToWindow = (winRef, htmlContent) => {
        //     if (winRef) {
        //         winRef.document.open();
        //         winRef.document.write(htmlContent);
        //         winRef.document.close();
        //     }
        // };

        window.openBlankWindow = () => {
            reportWindow = window.open("", "_blank");
        };

        window.writeHtmlToWindow = (htmlContent) => {
            if (reportWindow && !reportWindow.closed) {
                reportWindow.document.open();
                reportWindow.document.write(htmlContent);
                reportWindow.document.close();
            }
        };
    </script>
</body>

